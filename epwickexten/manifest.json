{
    "manifest_version" : 3,
    "name": "get back to work",
    "description" : "always get distract by social media?",
    "version": "1.0",
    "permissions" : ["tabs" , "activeTab"],
    "action": {
        "default_popup" : "popup.html",
        "default_icon" : "icon.png"
    },
    "chrome_url_overrides" : {
        "newtab" : "new.html"
    },
    "background" : {
        "service_worker" : "content.js"
    }
}